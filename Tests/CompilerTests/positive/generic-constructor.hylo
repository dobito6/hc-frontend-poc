struct A<T> {
  memberwise init
  fun f(x: sink T) -> T { x }
}

struct B<T, U> {
  memberwise init
  let x: T
  let y: U
}

fun check<T>(_ x: T) {}

fun main() {
  let a0 = A<Void>()
  check<A<Void>>(a0)

  let a1 = A().f(x: ())
  check<Void>(a1)

  let b0 = B(x: (x: ()), y: (y: ()))
  check<B<{x: Void}, {y: Void}>>(b0)
}
