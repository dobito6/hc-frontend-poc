trait P { fun g() }

struct A<T> { fun g() {} }
struct B<T> {}
struct C<T> {}

// Requirement implemented in primary declaration.
given <T> A<T>: P {}

// Requirement implemented in the conformance declaration.
given <T> B<T>: P {
  fun g() {}
}

// Requirement implemented in extension.
given <T> C<T>: P {}
extension <T> C<T> {
  fun g() {}
}
